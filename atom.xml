---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>{{ site.title }}</title>
 <link href="http://madebynathan.com/atom.xml" rel="self"/>
 <link href="http://madebynathan.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>madebynathan.com</id>

 {% if site.author || site.email %}
 <author>
   <name>{{ site.author }}</name>
   <email>{{ site.email }}</email>
 </author>
 {% endif %}

 {% for post in site.posts %}
 <entry>
   <title><![CDATA[{{ post.title }}]]></title>
   <link href="http://madebynathan.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>madebynathan.com{{ post.id }}</id>
   <summary><![CDATA[
    {% if post.excerpt %}{{ post.excerpt }}{% else %}{{ post.tags | join " " }}{% endif %}
   ]]></summary>
   <content type="html"><![CDATA[{{ post.content }}]]></content>

{% comment %}
 Pending implementation
 {% for category in post.categories %}
   <category scheme="http://madebynathan.com/category/" term="{{ category | downcase }}" label="{{ category }}" />
 {% endfor %}
 {% for tag in post.tags %}
   <category scheme="http://madebynathan.com/tag/" term="{{ tag | downcase }}" label="{{ tag }}" />
 {% endfor %}
{% endcomment %}

 </entry>
 {% endfor %}

</feed>

